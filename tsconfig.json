{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react",
    "sourceMap": true,
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "files": ["src/index.tsx"],
  "include": ["src","index.d.ts"],
  "exclude": ["node_modules", "**/__tests__/*"]
}
